#include "SLinkedList.h"

int main()
{
  SLinkedList<int> list;
  for(int i = 0; i < 10; ++i)
  {
    list.pushFront(i);
  }
  list.print_all();
  std::cout<<std::endl<<list.TopFront();
  std::cout<<std::endl<<list.getSize()<<std::endl;
  list.PopFront();
  list.print_all();
  std::cout<<std::endl<<list.TopFront();
  std::cout<<std::endl<<list.getSize();
  list.pushBack(0);
  list.print_all();
  return 0;
}
